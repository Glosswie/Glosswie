using System;
public class scarchEvenNumber
{
    
    public static void Main(string []arg)
        {
          int total = 20;
          int [] numbers =new int [total];
          for(int i=0; i<total; i++)
          {
        numbers[i] = Convert.ToInt32(Console.ReadLine());
          }
          for (int i=0; i<total; i++)
            { 
         if (numbers[i] % 2==0)
        {
        Console.Write("{0} ", numbers[i]);
         }
        }
    }
}
